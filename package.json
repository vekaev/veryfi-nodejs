{
  "name": "@veryfi/veryfi-sdk",
  "version": "1.2.2",
  "description": "Node.js module for communicating with the Veryfi OCR API",
  "main": "dist/lib/main.js",
  "typings": "dist/lib/types.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "npx husky install",
    "test": "jest",
    "doc": "jsdoc -d docs --configure jsconf.json main.js",
    "generate-docs": "jsdoc --configure jsdoc.json --verbose",
    "build": "rm -rf dist && tsc"
  },
  "author": "Veryfi <support@veryfi.com> (https://www.veryfi.com)",
  "homepage": "https://veryfi.github.io/veryfi-nodejs/",
  "contributors": [
    "Matthew Eng <matthew@veryfi.com>",
    "Alejandro Uribe <alejandro@veryfi.com>"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "readmeFilename": "README.md",
  "devDependencies": {
    "@jest/globals": "29.6.1",
    "@types/node": "20.4.2",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "docdash": "2.0.1",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "jest-config": "29.6.1",
    "jsdoc": "4.0.2",
    "lint-staged": "13.2.3",
    "minami": "1.2.3",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "axios": "1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veryfi/veryfi-nodejs.git"
  },
  "keywords": [
    "nodejs",
    "api",
    "ocr",
    "receipt",
    "invoice",
    "sdk-nodejs",
    "veryfi",
    "veryfi-api"
  ],
  "bugs": {
    "url": "https://github.com/veryfi/veryfi-nodejs/issues"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run lint:fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  }
}
